{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDB"
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Instrument": {
    "dataSource": "mongoDB",
    "public": true
  },
  "user": {
    "dataSource": "mongoDB",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "InstrumentTracking": {
    "dataSource": "mongoDB",
    "public": true
  },
  "Rate": {
    "dataSource": "mongoDB",
    "public": true
  }
}
